					</Anchor>
				</Anchors>
				<Scripts>
					<OnMouseDown>
						OQ_ResizeMouse_down( self ) ;
					</OnMouseDown>
					<OnLoad>
						self:GetNormalTexture():SetVertexColor(0.6, 0.6, 0.6)
					</OnLoad>
					<OnMouseUp>
						OQ_ResizeMouse_up( self ) ;
					</OnMouseUp>
					<OnHide>
						self:GetParent():StopMovingOrSizing()
					</OnHide>
				</Scripts>
				<NormalTexture file="Interface\AddOns\oqueue\art\resize"/>
			</Button>
			
