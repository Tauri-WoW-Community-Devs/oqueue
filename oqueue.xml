           <Button name="$parentTab1" inherits="CharacterFrameTabButtonTemplate" id="1" text= "Premade" >
               <Anchors>
                   <Anchor point="CENTER" relativePoint="BOTTOMLEFT">
                       <Offset>
                           <AbsDimension x="60" y="-12"/>
                       </Offset>
                   </Anchor>
               </Anchors>
               <Scripts>
                   <OnClick>
                       PanelTemplates_SetTab(OQMainFrame, 1);
                       OQ_hide_menu_popup() ;
                       OQTabPage1:Show();
                       OQTabPage2:Hide();
                       OQTabPage3:Hide();
                       OQTabPage4:Hide();
